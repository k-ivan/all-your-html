{
  "manifest_version": 2,
  "name": "FPS Meter",
  "version": "0.1.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/webcomponents-sd-ce.js",
        "js/fps.js",
        "js/content.js"
      ],
      "css": ["css/fps.css"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": ["js/background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png"
  }
}