{
  "manifest_version": 2,
  "name": "FPS Meter",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["js/fps.js"],
      "css": ["css/fps.css"]
    }
  ],
  "background": {
    "scripts": ["js/background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png"
  }
}